<footer style="background: {{ settings.footer_bg }}; color: {{ settings.footer_text_color }};">
	{{ settings.footer_text }}
</footer>

{% schema %}
{
	"name": "Custom Section",
	"settings": [
		{
			"type": "text",
			"id": "footer",
			"label": "Footer",
			"default": "footer"
		}
	],
	"blocks": [],
	"max_blocks": 1,
	"presets": [
		{
			"name": "Footer Section",
			"settings": {
				"footer": "footer"
			}
		}
	]
}
{% endschema %}
